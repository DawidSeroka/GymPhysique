<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.SCHEDULE_EXACT_ALARM" />

    <application
            android:name="com.myproject.gymphysique.GymPhysiqueApp"
            android:allowBackup="true"
            android:dataExtractionRules="@xml/data_extraction_rules"
            android:fullBackupContent="@xml/backup_rules"
            android:icon="@mipmap/ic_application"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_application_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.App.Startson"
            tools:targetApi="s">
        <activity
                android:name="com.myproject.gymphysique.MainActivity"
                android:exported="true"
                android:configChanges="orientation"
                android:screenOrientation="portrait"
                android:theme="@style/Theme.App.Startson">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
                android:name="com.myproject.gymphysique.accountsetup.receiver.AlarmReceiver"
                android:enabled="true" />
    </application>

</manifest>